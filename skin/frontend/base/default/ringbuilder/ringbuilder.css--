.ringbuilder.slide_show.mobile-cat-list{
 display: none;
}
.ringbuilder.slide_show.desktop-cat-list {
  position: relative;
  width: 100%;
}
.ringbuilder .liststep {
  max-width: 909px;
  padding: 0 10px;margin: 0 auto;
}
.ringbuilder.liststep li{
  display: inline-block;
}
.ringbuilder.slide_show.desktop-cat-list .liststep ol {
  position: absolute;
  bottom: 0;
}
.ringbuilder .liststep li.se-diamond {
  /* background: url(../images/selectdiamomd1st.png); */
  background: url(../images/1stdiamond.png);
  background-repeat: no-repeat;
  background-position-x: 2px;
   font-style: italic;
  padding: 10px 0px 9px 44px;
  float: left;
  width: 303px;
  position: relative;
  font-size: 14px;
  height: 84px;
  margin: 0;
}
.ringbuilder .liststep li.se-diamond{
 background: url(../images/1stdiamond_1.png);
  background-repeat: no-repeat;
