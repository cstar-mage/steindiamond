  padding: 10px 0px 9px 44px;
  float: left;
  width: 299px;
  position: relative;
  font-size: 14px;
  height: 84px;
  margin: 0;
}
.ringbuilder .liststep li.re-ring.selected{
background: url(../images/2nddiamond_1.png);
}
.ringbuilder .liststep li.checkout {
  background: url(../images/3rdunselected.png);
  background-position-x: -3px;
  background-repeat: no-repeat;
  padding: 10px 0px 9px 44px;
  font-style: italic;
  float: left;
  width: 301px;
  position: relative;
  font-size: 14px;
  height: 84px;
  margin: 0;
}
.ringbuilder .liststep li.checkout.selected{
background: url(../images/3rdunselected+_1.png);
}
.ringbuilder .liststep li a, .ringbuilder .liststep li a:hover {
  vertical-align: middle;
  font-size: 16px;
  font-weight: 400;
  color: #fff !important;
  font-family: 'Playfair Display', serif;
  font-style: normal;
  text-transform: uppercase;
}
.ringbuilder .liststep li.selected a, .ringbuilder .liststep li.selected a:hover {
  vertical-align: middle;
  font-size: 16px;
  font-weight: 400;
  color: #41395f !important;
  font-family: 'Playfair Display', serif;
  font-style: normal;
  text-transform: uppercase;
}


.ringbuilder .steps-div a{
font-size: 12px !important;
  color: #fff !important;
  font-style: italic !important;
  padding-right: 5%;
  text-transform: capitalize !important;
  text-decoration: underline !important;
  }
.ringbuilder .liststep li.selected span {
  position: absolute;
  bottom: 0;
  left: 6px;
  top: 0px;
  right: unset;
  color: #41395f;
  font-size: 60px;
  line-height: 1;
  font-family: 'Playfair Display', serif;
}

.ringbuilder .liststep li.re-ring > a {
